TARGET = $(subst 64,x86_64,$(subst 32,i686,$(WIN)))-pc-windows-gnu
LIBDIR = ./rust/target/$(TARGET)/release
STATLIB = $(LIBDIR)/libbytetrunc.a
PKG_LIBS = -L$(LIBDIR) -lbytetrunc -lws2_32 -ladvapi32 -luserenv -lbcrypt

all: C_clean

C_clean:
	rm -Rf $(SHLIB) $(OBJECTS)

clean:
	rm -Rf $(SHLIB) $(OBJECTS)
